{
  "addons": [
    "memcachedcloud",
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "env": {
    "display_domain": {
      "required": true
    },
    "domain": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "MEMCACHEDCLOUD_PASSWORD": {
      "required": true
    },
    "MEMCACHEDCLOUD_SERVERS": {
      "required": true
    },
    "MEMCACHEDCLOUD_USERNAME": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "secret_key_base": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "name": "dinshaw-cv",
  "scripts": {
  },
  "stack": "heroku-18"
}
